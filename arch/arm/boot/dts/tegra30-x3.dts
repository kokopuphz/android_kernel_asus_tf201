						"lcd_d4_pe4",		/* LCD_RGB_RGB_B_6   */
						"lcd_d5_pe5",		/* LCD_RGB_RGB_B_7   */
						"lcd_d6_pe6",		/* LCD_RGB_RGB_G_2   */
						"lcd_d7_pe7",		/* LCD_RGB_RGB_G_3   */
						"lcd_d8_pf0",		/* LCD_RGB_RGB_G_4   */
						"lcd_d9_pf1",		/* LCD_RGB_RGB_G_5   */
						"lcd_d10_pf2",		/* LCD_RGB_RGB_G_6   */
						"lcd_d11_pf3",		/* LCD_RGB_RGB_G_7   */
