				nvidia,pull = <1>;			/* PULL_DOWN  *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};

			/* DAP3: UNKNOWN */
			dap3_fs_pp0 {
				nvidia,pins =	"dap3_fs_pp0",		/* UNKNOWN          */
						"dap3_din_pp1",		/* UNKNOWN          */
						"dap3_dout_pp2",	/* UNKNOWN          */
						"dap3_sclk_pp3";	/* UNKNOWN          */
				nvidia,function = "i2s2";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};

			/* DAP4 I2S3: Bluetooth BCM4330 PCM I/F */
			dap4_din_pp1 {
				nvidia,pins =	"dap4_din_pp1",		/* BT_PCM_DIN       */
