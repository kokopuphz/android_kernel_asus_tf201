						"lcd_d15_pf7",		/* LCD_RGB_RGB_R_5   */
						"lcd_d16_pm0",		/* LCD_RGB_RGB_R_6   */
						"lcd_d17_pm1",		/* LCD_RGB_RGB_R_7   */
						"lcd_d18_pm2",		/* LCD_RGB_RGB_B_0   */
						"lcd_d19_pm3",		/* LCD_RGB_RGB_B_1   */
						"lcd_d20_pm4",          /* LCD_RGB_RGB_G_0   */
						"lcd_d21_pm5",		/* LCD_RGB_RGB_G_1   */
						"lcd_d22_pm6",		/* LCD_RGB_RGB_R_0   */
						"lcd_d23_pm7",		/* LCD_RGB_RGB_R_1   */
						"lcd_pclk_pb3",		/* LCD_RGB_PCLK      */
						"lcd_de_pj1",		/* LCD_RGB_DE        */
						"lcd_hsync_pj3",	/* LCD_RGB_HSYNC     */
						"lcd_vsync_pj4",	/* LCD_RGB_VSYNC     */
						"lcd_wr_n_pz3";		/* UNKNOWN           */
				nvidia,function = "displaya";
				nvidia,pull = <0>;			/* NORMAL     *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
