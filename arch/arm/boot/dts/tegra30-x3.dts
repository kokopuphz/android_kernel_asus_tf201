/ {
	model = "EternityProject Tegra30 X3 Phone";
	compatible = "nvidia,x3", "nvidia,tegra30";

	memory {
		reg = <0x80000000 0x80000000>;
	};

	pinmux {
		pinctrl-names = "default";
		pinctrl-0 = <&state_default>;

		state_default: pinmux {

			/* I2C MUX */
			gen1_i2c_scl_pc4 {
				nvidia,pins =	"gen1_i2c_scl_pc4",	/* UNKNOWN           */
						"gen1_i2c_sda_pc5";	/* UNKNOWN           */
				nvidia,function = "i2c1";
				nvidia,pull = <0>;			/* NORMAL     *P*/
				nvidia,tristate = <0>;			/* NORMAL     *S*/
				nvidia,enable-input = <1>;
				nvidia,lock = <0>;			/* UNLOCKED   *L*/
				nvidia,open-drain = <0>;		/* DISABLED   *D*/
			};

			gen2_i2c_scl_pt5 {
				nvidia,pins =	"gen2_i2c_scl_pt5",	/* MUIC_INIT         */
						"gen2_i2c_sda_pt6";	/* MUIC_INIT         */
				nvidia,function = "i2c2";
				nvidia,pull = <2>;			/* PULL_UP    *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
				nvidia,lock = <0>;
				nvidia,open-drain = <0>;
			};

			cam_i2c_scl_pbb1 {
				nvidia,pins =	"cam_i2c_scl_pbb1",	/* CAM_I2C_SCL       */
						"cam_i2c_sda_pbb2";	/* CAM_I2C_SDA       */
				nvidia,function = "i2c3";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
				nvidia,lock = <0>;
				nvidia,open-drain = <0>;
			};

			ddc_scl_pv4 {
				nvidia,pins =	"ddc_scl_pv4",		/* HDMI_EDID         */
						"ddc_sda_pv5";		/* HDMI_EDID         */
				nvidia,function = "i2c4";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
				nvidia,lock = <0>;
				nvidia,open-drain = <0>;
			};

			pwr_i2c_scl_pz6 {
				nvidia,pins =	"pwr_i2c_scl_pz6",	/* UNKNOWN           */
						"pwr_i2c_sda_pz7";	/* UNKNOWN           */
				nvidia,function = "i2cpwr";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
				nvidia,lock = <0>;
				nvidia,open-drain = <0>;
			};

			/* 32k clock OUT: !!NC!! */
			clk_32k_out_pa0 {
				nvidia,pins = "clk_32k_out_pa0";	/* UNKNOWN           */
				nvidia,function = "blink";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>; /* Why the hell?! */
			};

			/* SDMMC3: TI WL1283 SDIO */
			sdmmc3_clk_pa6 {
				nvidia,pins = "sdmmc3_clk_pa6";		/* MICROSD_CLK       */
				nvidia,function = "sdmmc3";		/* SDMMC3 MUX *M*/
				nvidia,pull = <0>;			/* NORMAL     *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};
			sdmmc3_cmd_pa7 {
				nvidia,pins =	"sdmmc3_cmd_pa7",	/* MICROSD_CMD       */
						"sdmmc3_dat0_pb7",	/* MICROSD_DAT0      */
						"sdmmc3_dat1_pb6",	/* MICROSD_DAT1      */
						"sdmmc3_dat2_pb5",	/* MICROSD_DAT2      */
						"sdmmc3_dat3_pb4";	/* MICROSD_DAT3      */
				nvidia,function = "sdmmc3";		/* SDMMC3 MUX *M*/
				nvidia,pull = <2>;			/* PULL_UP    *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};

			/* SDMMC4: EMMC */
			sdmmc4_clk_pcc4 {
				nvidia,pins =	"sdmmc4_clk_pcc4";	/* eMMC_CLK          */
				nvidia,function = "sdmmc4";
				nvidia,pull = <0>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};
			sdmmc4_rst_n_pcc3 {
				nvidia,pins =	"sdmmc4_rst_n_pcc3";	/* eMMC_RST_N        */
				nvidia,function = "rsvd1";
				nvidia,pull = <1>;			/* PULL_DOWN  *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};
			sdmmc4_cmd_pt7 {
				nvidia,pins =	"sdmmc4_cmd_pt7",	/* eMMC_CMD          */
						"sdmmc4_dat0_paa0",	/* eMMC_DAT0         */
						"sdmmc4_dat1_paa1",	/* eMMC_DAT1         */
						"sdmmc4_dat2_paa2",	/* eMMC_DAT2         */
						"sdmmc4_dat3_paa3",	/* eMMC_DAT3         */
						"sdmmc4_dat4_paa4",	/* eMMC_DAT4         */
						"sdmmc4_dat5_paa5",	/* eMMC_DAT5         */
						"sdmmc4_dat6_paa6",	/* eMMC_DAT6         */
						"sdmmc4_dat7_paa7";	/* eMMC_DAT7         */
				nvidia,function="sdmmc4";
				nvidia,pull = <2>;			/* PULL_UP    *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};

			/* UART2: UNKNOWN */
			uart2_txd_pc2 {
				nvidia,pins =	"uart2_txd_pc2";	/* UNKNOWN         */
				nvidia,function = "irda";
				nvidia,pull = <2>;
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
