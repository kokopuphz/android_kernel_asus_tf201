		spi-max-frequency = <10800000>;
		/* AIC3008 is on SPI1, bus_num 1 */
		/* Define AIC3008 parameters here, please. */
	};

	/* SPI4? */
	spi@7000dc00 {
		status = "okay";
		spi-max-frequency = <25000000>;
		/* HTC rawchip is on SPI4, bus_num 3 */
		/* Define RAWCHIP parameters here, please. */
	};

	clocks {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		clk32k_in: clock {
			compatible = "fixed-clock";
			reg=<0>;
			#clock-cells = <0>;
			clock-frequency = <32768>;
		};
	};

	/* tegra-snd-aic3008 // AIC3008 chip */
	sound {
		compatible = "nvidia,eprj-audio-tlv320aic3008-endeavoru",
			     "nvidia,eprj-audio-tlv320aic3008";
		nvidia,model = "EternityProject TLV320AIC3008 Audio for ENRU";

/*		nvidia,i2s-controller = <&tegra_i2s1>;*/
/*		nvidia,audio-codec = <&aic3008>;*/
	};
};
