				nvidia,pull = <0>;			/* NORMAL     *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <1>;
			};

			/* SPDIF CONTROL */
			spdif_out_pk5 {
				nvidia,pins =	"spdif_out_pk5";	/* NC                */
