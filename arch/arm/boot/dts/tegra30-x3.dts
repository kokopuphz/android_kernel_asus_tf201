				nvidia,pins =	"uart3_txd_pw6",	/* BT_RX_AP_TX     */
						"uart3_rts_n_pc0";	/* BT_CTS_AP_RTS   */
				nvidia,function = "uartc";		/* UARTC MUX  *M*/
				nvidia,pull = <0>;			/* NORMAL     *P*/
				nvidia,tristate = <0>;
				nvidia,enable-input = <0>;
