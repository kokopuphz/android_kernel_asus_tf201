	ahub {
		i2s@70080400 {
			status = "okay";
		};
	};

	pmc {
		status = "okay";
		nvidia,invert-interrupt;
		nvidia,suspend-mode = <0>;
		nvidia,cpu-pwr-good-time = <600>;
		nvidia,cpu-pwr-off-time = <200>;
		nvidia,core-pwr-good-time = <2849 2849>;
		nvidia,core-pwr-off-time = <0>;
		nvidia,core-power-req-active-high;
		nvidia,sys-clock-req-active-high;
		nvidia,lp0-vec = <0xbc0a8000 0x2000>;
	};

	/* TI WL1283 SDIO */
	sdhci@78000400 {
		status = "okay";
		bus-width = <4>;
		broken-cd; /* There is no card detection available; polling must be used??? */
	};

	/* EMMC */
	sdhci@78000600 {
		status = "okay";
		bus-width = <8>;
		non-removable; /* as it is an eMMC, obviously */
	};

	serial@70006000 {
		status = "okay";
	};

	serial@70006200 {
		compatible = "nvidia,tegra30-hsuart";
		status = "okay";
	};

	/* i2c1 */
	i2c@7000c000 {
		status = "okay";
		clock-frequency = <384000>;

		/* TI TLV320AIC3008 is on SPI1 */
	};

	/* i2c2 */
	i2c@7000c400 {
		status = "okay";
		clock-frequency = <384000>;
	};

	/* i2c3 */
	i2c@7000c500 {
		status = "okay";
		clock-frequency = <384000>;
	};

	/* i2c4 */
	i2c@7000c700 {
		status = "okay";
		clock-frequency = <10000>;
	};

	/* i2c5 */
	i2c@7000d000 {
		status = "okay";
		clock-frequency = <100000>;
	};

	/* SPI1? */
	spi@7000d600 {
		status = "okay";
		spi-max-frequency = <10800000>;
		/* AIC3008 is on SPI1, bus_num 1 */
		/* Define AIC3008 parameters here, please. */
	};

	/* SPI4? */
	spi@7000dc00 {
		status = "okay";
		spi-max-frequency = <25000000>;
		/* HTC rawchip is on SPI4, bus_num 3 */
		/* Define RAWCHIP parameters here, please. */
	};

	clocks {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		clk32k_in: clock {
			compatible = "fixed-clock";
			reg=<0>;
			#clock-cells = <0>;
			clock-frequency = <32768>;
		};
	};

